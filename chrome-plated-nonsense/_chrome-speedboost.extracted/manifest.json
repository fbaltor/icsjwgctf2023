{
    "manifest_version": 3,
    "name": "Chrome Speed Booster",
    "description": "Boosts Chrome page loading speed - make Chrome faster!",
    "version": "1.0",
    "permissions": [
        "tabs", "webNavigation", "system.cpu", "system.memory"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "logo_16.png",
        "32": "logo_32.png",
        "48": "logo_48.png",
        "128": "logo_128.png"
    }
  }